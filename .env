# 开发环境配置
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:5173

# PostgreSQL数据库配置（开发环境）
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=huigei_dev
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password

# JWT配置
JWT_SECRET=dev-jwt-secret-key-change-in-production
JWT_EXPIRES_IN=7d

# 云存储配置（开发环境使用模拟）
STORAGE_PROVIDER=aliyun

# 阿里云OSS配置（开发环境示例）
ALIYUN_OSS_ACCESS_KEY_ID=your-dev-access-key-id
ALIYUN_OSS_ACCESS_KEY_SECRET=your-dev-access-key-secret
ALIYUN_OSS_REGION=oss-cn-hangzhou
ALIYUN_OSS_BUCKET=huigei-dev-bucket
ALIYUN_OSS_ENDPOINT=oss-cn-hangzhou.aliyuncs.com

# 腾讯云COS配置（开发环境示例）
TENCENT_COS_SECRET_ID=your-dev-secret-id
TENCENT_COS_SECRET_KEY=your-dev-secret-key
TENCENT_COS_REGION=ap-beijing
TENCENT_COS_BUCKET=huigei-dev-bucket-1234567890